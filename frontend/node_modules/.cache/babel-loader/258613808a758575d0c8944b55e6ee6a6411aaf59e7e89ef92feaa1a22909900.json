dio value'\n      },\n      inline: {\n        type: Boolean,\n        description: 'Whether radio is inline'\n      }\n    },\n    data() {\n      return {\n        cbId: ''\n      };\n    },\n    computed: {\n      model: {\n        get() {\n          return this.value;\n        },\n        set(value) {\n          this.$emit('input', value);\n        }\n      },\n      inlineClass() {\n        if (this.inline) {\n          return `form-check-inline`;\n        }\n        return '';\n      }\n    },\n    created() {\n      this.cbId = Math.random()\n        .toString(16)\n        .slice(2);\n    }\n  };\n</script>\n"],"mappings":"AAoBE,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,KAAK,EAAE;IACLD,IAAI,EAAE;MACJE,IAAI,EAAE,CAACC,MAAM,EAAEC,MAAM,CAAC;MACtBC,WAAW,EAAE;IACf,CAAC;IACDC,QAAQ,EAAE;MACRJ,IAAI,EAAEK,OAAO;MACbF,WAAW,EAAE;IACf,CAAC;IACDG,KAAK,EAAE;MACLN,IAAI,EAAE,CAACC,MAAM,EAAEI,OAAO,CAAC;MACvBF,WAAW,EAAE;IACf,CAAC;IACDI,MAAM,EAAE;MACNP,IAAI,EAAEK,OAAO;MACbF,WAAW,EAAE;IACf;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,KAAK,EAAE;MACLC,GAAGA,CAAA,EAAG;QACJ,OAAO,IAAI,CAACN,KAAK;MACnB,CAAC;MACDO,GAAGA,CAACP,KAAK,EAAE;QACT,IAAI,CAACQ,KAAK,CAAC,OAAO,EAAER,KAAK,CAAC;MAC5B;IACF,CAAC;IACDS,WAAWA,CAAA,EAAG;MACZ,IAAI,IAAI,CAACR,MAAM,EAAE;QACf,OAAQ,mBAAkB;MAC5B;MACA,OAAO,EAAE;IACX;EACF,CAAC;EACDS,OAAOA,CAAA,EAAG;IACR,IAAI,CAACP,IAAG,GAAIQ,IAAI,CAACC,MAAM,CAAC,EACrBC,QAAQ,CAAC,EAAE,EACXC,KAAK,CAAC,CAAC,CAAC;EACb;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}