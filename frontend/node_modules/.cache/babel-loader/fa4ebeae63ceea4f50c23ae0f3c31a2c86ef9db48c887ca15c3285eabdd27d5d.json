ws\\HomeView.vue"],"sourcesContent":["<template>\r\n    <div class=\"home-screen\">\r\n      <div class=\"header\">\r\n        <select v-model=\"selectedCity\">\r\n          <option v-for=\"city in cities\" :value=\"city.id\" :key=\"city.id\">{{ city.name }}</option>\r\n        </select>\r\n        <div class=\"notification-icon\">\r\n          <i class=\"fas fa-bell\"></i>\r\n        </div>\r\n      </div>\r\n      <div class=\"events-carousel\">\r\n        <div v-for=\"event in events\" :key=\"event.id\" class=\"event-item\">\r\n          <div class=\"day-of-week\">{{ getDayOfWeek(event.date) }}</div>\r\n          <div class=\"day-of-month\">{{ getDayOfMonth(event.date) }}</div>\r\n        </div>\r\n      </div>\r\n      <div class=\"timeline\">\r\n        <div v-for=\"event in events\" :key=\"event.id\" class=\"event-card\">\r\n          <div class=\"event-image\">\r\n            <img :src=\"event.image\" alt=\"Event Image\" />\r\n          </div>\r\n          <div class=\"event-title\">{{ event.title }}</div>\r\n          <div class=\"event-location\">{{ event.location }}</div>\r\n          <div class=\"event-date\">{{ formatDate(event.date) }}</div>\r\n          <div class=\"event-price\">{{ event.price }}</div>\r\n          <div class=\"event-rating\">\r\n            <span class=\"rating-label\">Classificação:</span>\r\n            <div class=\"rating-stars\">\r\n            \r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        selectedCity: null,\r\n        cities: [\r\n          { id: 1, name: \"City A\" },\r\n          { id: 2, name: \"City B\" },\r\n          { id: 3, name: \"City C\" },\r\n        ],\r\n        events: [\r\n          {\r\n            id: 1,\r\n            title: \"Event 1\",\r\n            location: \"Location 1\",\r\n            date: \"2023-05-20\",\r\n            price: \"$10\",\r\n            rating: 4,\r\n            image: \"https://example.com/event1.jpg\",\r\n          },\r\n          {\r\n            id: 2,\r\n            title: \"Event 2\",\r\n            location: \"Location 2\",\r\n            date: \"2023-05-21\",\r\n            price: \"$15\",\r\n            rating: 5,\r\n            image: \"https://example.com/event2.jpg\",\r\n          },\r\n          {\r\n            id: 3,\r\n            title: \"Event 3\",\r\n            location: \"Location 3\",\r\n            date: \"2023-05-22\",\r\n            price: \"$20\",\r\n            rating: 3,\r\n            image: \"https://example.com/event3.jpg\",\r\n          },\r\n        ],\r\n      };\r\n    },\r\n    methods: {\r\n      getDayOfWeek(date) {\r\n        const daysOfWeek = [\"Sunday\", \"Monday\", \"Tuesday\", \"Wednesday\", \"Thursday\", \"Friday\", \"Saturday\"];\r\n        const eventDate = new Date(date);\r\n        return daysOfWeek[eventDate.getDay()];\r\n      },\r\n      getDayOfMonth(date) {\r\n        const eventDate = new Date(date);\r\n        return eventDate.getDate();\r\n      },\r\n      formatDate(date) {\r\n        const eventDate = new Date(date);\r\n        const options = { weekday: \"long\", month: \"short\", day: \"numeric\", hour: \"numeric\", minute: \"numeric\" };\r\n        return eventDate.toLocaleString(\"en-US\", options);\r\n    },\r\n  },\r\n};\r\n</script>\r\n  \r\n  <style scoped>  \r\n  @import url('@/assets/css/gategun.css');\r\n  </style>"],"mappings":"AAqCE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,IAAI;MAClBC,MAAM,EAAE,CACN;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE;MAAS,CAAC,EACzB;QAAED,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE;MAAS,CAAC,EACzB;QAAED,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE;MAAS,CAAC,CAC1B;MACDC,MAAM,EAAE,CACN;QACEF,EAAE,EAAE,CAAC;QACLG,KAAK,EAAE,SAAS;QAChBC,QAAQ,EAAE,YAAY;QACtBC,IAAI,EAAE,YAAY;QAClBC,KAAK,EAAE,KAAK;QACZC,MAAM,EAAE,CAAC;QACTC,KAAK,EAAE;MACT,CAAC,EACD;QACER,EAAE,EAAE,CAAC;QACLG,KAAK,EAAE,SAAS;QAChBC,QAAQ,EAAE,YAAY;QACtBC,IAAI,EAAE,YAAY;