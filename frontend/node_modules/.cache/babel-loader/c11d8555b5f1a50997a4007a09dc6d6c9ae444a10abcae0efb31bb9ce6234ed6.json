\r\n      PageVisitsTable,\r\n      SocialTrafficTable\r\n    },\r\n    data() {\r\n      return {\r\n        bigLineChart: {\r\n          allData: [\r\n            [0, 20, 10, 30, 15, 40, 20, 60, 60],\r\n            [0, 20, 5, 25, 10, 30, 15, 40, 40]\r\n          ],\r\n          activeIndex: 0,\r\n          chartData: {\r\n            datasets: [\r\n              {\r\n                label: 'Performance',\r\n                data: [0, 20, 10, 30, 15, 40, 20, 60, 60],\r\n              }\r\n            ],\r\n            labels: ['May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],\r\n          },\r\n          extraOptions: chartConfigs.blueChartOptions,\r\n        },\r\n        redBarChart: {\r\n          chartData: {\r\n            labels: ['Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],\r\n            datasets: [{\r\n              label: 'Sales',\r\n              data: [25, 20, 30, 22, 17, 29]\r\n            }]\r\n          },\r\n          extraOptions: chartConfigs.blueChartOptions\r\n        }\r\n      };\r\n    },\r\n    methods: {\r\n      initBigChart(index) {\r\n        let chartData = {\r\n          datasets: [\r\n            {\r\n              label: 'Performance',\r\n              data: this.bigLineChart.allData[index]\r\n            }\r\n          ],\r\n          labels: ['May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'],\r\n        };\r\n        this.bigLineChart.chartData = chartData;\r\n        this.bigLineChart.activeIndex = index;\r\n      }\r\n    },\r\n    mounted() {\r\n      this.initBigChart(0);\r\n    }\r\n  };\r\n</script>\r\n<style>\r\n.el-table .cell{\r\n  padding-left: 0px;\r\n  padding-right: 0px;\r\n}\r\n</style>\r\n"],"mappings":"AA0IE;AACA,OAAO,KAAKA,YAAW,MAAO,4BAA4B;AAC1D,OAAOC,SAAQ,MAAO,+BAA+B;AACrD,OAAOC,QAAO,MAAO,8BAA8B;;AAEnD;AACA,OAAOC,YAAW,MAAO,2BAA2B;AACpD,OAAOC,SAAQ,MAAO,8BAA8B;;AAEpD;AACA,OAAOC,kBAAiB,MAAO,gCAAgC;AAC/D,OAAOC,eAAc,MAAO,6BAA6B;AAEzD,eAAe;EACbC,UAAU,EAAE;IACVN,SAAS;IACTC,QAAQ;IACRC,YAAY;IACZC,SAAS;IACTE,eAAe;